{
  "metadata": [
    {
      "src": [
        {
          "files": [
            "HKMirror/**.csproj"
          ],
          "src": "..",
          "exclude": [
            "HKMirror/Hooks/ILHooks/*.cs",
            "HKMirror/Hooks/OnHooks/*.cs",
            "HKMirror/Reflections/InstanceClasses/Classes/*.cs",
            "HKMirror/Reflections/InstanceClasses/OtherClasses/*.cs",
			"HKMirror/Reflections/SingletonClasses/*.cs",
			"HKMirror/Reflections/StaticClasses/*.cs",
			"HKMirror/Reflections/Extensions.cs",
			"HKMirror/Generator/*.cs",
          ]
        }
      ],
      "dest": "api",
	  "filter": "filterConfig.yml",
      "disableGitFeatures": false,
      "disableDefaultFilter": false
    }
  ],
  "build": {
    "xrefService": [ "https://xref.docs.microsoft.com/query?uid={uid}" ],
    "content": [
      {
        "files": [
          "api/**.yml",
          "api/index.md"
        ]
      },
      {
        "files": [
          "articles/**.md",
          "articles/**/toc.yml",
          "toc.yml",
          "*.md"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "images/**"
        ]
      }
    ],
    "overwrite": [
      {
        "files": [
          "apidoc/**.md"
        ],
        "exclude": [
          "obj/**",
          "_site/**"
        ]
      }
    ],
    "dest": "_site",
    "globalMetadataFiles": [],
    "fileMetadataFiles": [],
    "template": [
      "default"
    ],
    "postProcessors": [],
    "markdownEngineName": "markdig",
    "noLangKeyword": false,
    "keepFileLink": false,
    "cleanupCacheHistory": false,
    "disableGitFeatures": false
  }
}